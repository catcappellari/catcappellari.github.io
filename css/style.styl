/*[Global Styles]*/
html {
    scroll-behavior: smooth;
}

@media (prefers-reduced-motion: reduce) {
    html {
        scroll-behavior: auto;
    }
}
body {
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    font-family: 'Gudea', sans-serif;
    font-size: 1.125rem;
    box-sizing: border-box;
}
body * {
    box-sizing: inherit;
}
h1,
h2,
h3,
h4,
h5,
h6 {
    font-family: 'Palanquin Dark', Arial Black, Arial Bold, Gadget, sans-serif;
    margin: 0;
    padding: 0.25em 0 0.5em;
    line-height: 1.2;
}
p,
a {
    font-family: 'Gudea', Tahoma, Verdana, Segoe, sans-serif;
    margin: 0;
    padding: 0.25em 0 0.5em;
    line-height: 1.2;
    font-weight: 400;
}
h1, .h1 {
    font-size: 2.074rem;
}
h2, .h2 {
    font-size: 1.728rem;
}
h3, .h3 {
    font-size: 1.44rem;
}
h4, .h4 {
    font-size: 1.2rem;
}
h5, .h5 {
    font-size: 1rem;
}
small {
    font-size: 0.833rem;
}
img {
    max-width: 100%;
    height: auto;
}


/*[Reusable Styles]*/

.clearfix:after {
    content: "";
    display: table;
}
.focus-only {
    position:absolute; 
    left:-10000px; 
    top:auto; 
    width:1px; 
    height:1px; 
    overflow:hidden;
    margin: 0 auto;
    display: block;
    text-align: center;
}
.focus-only:focus {
    position:static; 
    width:auto; 
    height:auto; 
}
.top .focus-only, .top .focus-only:hover {
    color: #fff;
}
.sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0,0,0,0);
    border: 0;
}
[tabindex="-1"]:focus {
    outline: 0;
}
.c-arrow-jump {
    display: block;
    box-sizing: border-box;
    height: 40px;
    width: 40px;
    margin: 1rem auto;
    background-size: 80px;
    background-repeat: no-repeat;
    background-image: url('../images/arrow-down-white.svg');
    background-position: center;
    padding: 0;
    opacity: 1;
    transition: opacity 0.33s linear;
}
.c-arrow-jump:hover {
    opacity: 0.5;
}
.top {
    background: #1f3a93;
    position: relative;
    box-sizing: border-box;
}
.index .top {
    background: linear-gradient(to top, #217cb8 0%, #1f3a93 100%);
    padding-bottom: 11vw;
}
.top-svg {
    position: absolute;
    bottom: -1px;
    left: 0;
    right: 0;
    height: 11vw;
    max-height: 200px;
    overflow: hidden;
}
.top-svg-image {
    height: 11vw;
    max-height: 200px;
    width: 100%;
}
.header-main {
    box-sizing: border-box;
    padding: 0.75rem;
    z-index: 20;
    position: relative;
    font-size: 0;
}
.header-main .h-block {
    display: inline-block;
    vertical-align: middle;
    width: 45%;
    text-align: left;
    font-size: 1rem;
}
.header-main .h-block:nth-child(2) {
    text-align: right;
    z-index: 10;
    width: 55%;
}
.header-main .logo {
    display: block;
    max-width: 35px;
    margin-left: 0;
    margin-right: 0;
    padding: 0;
    img {
        display: block;
        height: auto;
        width: 100%;
    }
}
.header-main .mainmenu {
    display: inline-block;
    vertical-align: middle;
}
.header-main .email {
    display: inline-block;
    vertical-align: middle;
    font-size: 2rem;
    margin-right: 0.5rem;
    color: #fff;
    text-decoration: none;
    padding: 0;
}


/* dropdown menu */
.menu-button {
  display: block;
  height: 32px;
  width: 32px;
  margin-left: auto;
  background: transparent;
  border: none;
  border-radius: 0;
  position: relative;
}
.menu-button span {
  display: block;
  position: absolute;
  height: 2px;
  width: 100%;
  left: 0;
  content: "";
  background: #fff;
  transition: all 0.5s;
}
.menu-button span:nth-child(1) {
  top: 0px;
}
.menu-button span:nth-child(2) {
  top: 15px;
  width: 25px;
  opacity: 1;
}
.menu-button span:nth-child(3) {
  bottom: 0px;
}

.menu-button.active span:nth-child(1) {
    top: 50%;
    -webkit-transform: rotate(45deg) translateY(-50%);
    transform: rotate(45deg) translateY(-50%);
}
.menu-button.active span:nth-child(2) {
    opacity: 0;
}
.menu-button.active span:nth-child(3) {
    bottom: 50%;
    -webkit-transform: rotate(-45deg) translateY(50%);
    transform: rotate(-45deg) translateY(50%);
}
.mobile-menu {
    box-sizing: border-box;
    padding: 0;
    margin: 0;
    list-style: none;
    background: rgba(255,255,255,0.95);
    position: absolute;
    top: 70px;
    right: -320px;
    width: 100%;
    max-width: 320px;
    transition: right 0.5s;
    &.active {
        right: 0;
    }
    a {
        display: block;
        box-sizing: border-box;
        padding: 0.75rem 1rem;
        border-bottom: 2px solid #217cb8;
        color: #1f3a93;
        text-decoration: none;
        font-weight: 700;
        text-transform: uppercase;
        &:hover {
            background-color: #fff;
        }
    }
}
.js-active {
    .mobile-menu {
        display: none;
        &.active {
            display: block;
        }
    }
}

/*[Section Styles]*/

.c-section {
    box-sizing: border-box;
    padding: 2rem 1rem;
        &_center {
        text-align: center;
    }
    &_dark {
        color: #fff;
        a, a:hover {
            color: #fff;
        }
    }   
    &_image {
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center;
        position: relative;
        &:after {
            position: absolute;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            content: "";
            background: rgba(0, 0, 0, 0.45);
        }
    } 
    &_alt1 {
        background-color: #efefef;
    }
    &_alt2 {
        background-color: #217cb8;
    }
    &_alt3 {
        background-color: #1f3a93;
    }
    &_alt4 {
        background-color: #000;
    }
    &_wide {
        padding-left: 0;
        padding-right: 0;
    }
    &_nopad {
        padding: 0;
    }
    &_tall {
        padding: 2.5rem 1rem;
    }
}
.c-contain {
    max-width: 960px;
    margin: 0 auto;
    position: relative;
    z-index: 5;
    &_660 {
        max-width: 660px;
    }
    &_1200 {
        max-width: 1200px;
    }
    &_1400 {
        max-width: 1400px;
    }
}
.c-pad {
    box-sizing: border-box;
    padding: 1.5rem 1rem;
}
.c-table {
    display: table;
    table-layout: fixed;
    width: 100%;
    &__cell {
        display: table-cell;
        vertical-align: middle;
        width: 100%;
        &_top {
            vertical-align: top;
        }
        &_bottom {
            vertical-align: bottom;
        }
    }
}
.c-slide {
    &__image {
        display: block;
        margin: 1.25rem auto;
    }
}
.c-buttons {
    box-sizing: border-box;
    padding: 1.5rem 0 0;
    margin: 0;
    text-align: center;
    li {
        display: block;
        box-sizing: border-box;
        padding: 0.5rem;
    }
    a {
        display: block;
        box-sizing: border-box;
        padding: 0.5rem 1.25rem;
        border: 2px solid #217cb8;
        width: 250px;
        max-width: 100%;
        margin: 0 auto;
        text-decoration: none;
        text-align: center;
        color: #217cb8;
        font-size: 18px;
        font-weight: bold;
        transition: all 0.3s linear;
        &:hover {
            background: #217cb8;
            color: #fff;
        }
    }
    &_light {
        a {
            border-color: #fff;
            color: #fff;
            &:hover {
                border-color: #217cb8;
            }
        }
    }
}
.c-list {
    display: inline-block;
    vertical-align: top;
    width: 100%;
    max-width: 400px;
    box-sizing: border-box;
    padding: 1rem 0;
    ul {
        margin: 0;
        text-align: left;
        padding: 0.5rem 0 0 2rem;
    }
    li {
        padding-bottom: 0.5em;
    }
}
.c-slick-dots {
    .slick-dots {
        box-sizing: border-box;
        padding: 1rem 1rem 1.5rem;
        margin: 0;
        list-style: none;
        li {
            display: inline-block;
            vertical-align: middle;
            width: 20px;
            height: 20px;
            margin: 10px;
            box-sizing: border-box;
            border: 3px solid #1f3a93;
            border-radius: 50%;
            cursor: pointer;
            position: relative;
        }
        .slick-active:after {
            position: absolute;
            top: 0;
            right: 0;
            width: 100%;
            height: 100%;
            content: "";
            background-color: #1f3a93;
        }
        button {
            position: absolute;
            width: 1px;
            height: 1px;
            padding: 0;
            margin: -1px;
            overflow: hidden;
            clip: rect(0,0,0,0);
            border: 0;
        }
    }
}
.c-pad {
    box-sizing: border-box;
    padding: 1rem;
    &_top {
        padding: 1rem 0 0;
    }
}
.c-flex {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-wrap: wrap;
        flex-wrap: wrap;
    -webkit-box-pack: center;
        -ms-flex-pack: center;
            justify-content: center;
    &_center {
        -webkit-box-align: center;
        -ms-flex-align: center;
            align-items: center;
    }
    &_top {
        -webkit-box-align: start;
        -ms-flex-align: start;
            align-items: flex-start;
    }
    &_nowrap {
        -ms-flex-wrap: nowrap;
        flex-wrap: nowrap;
    }
    &__item {
        -webkit-box-flex: 1;
        -ms-flex: 1 1 auto;
        flex: 1 1 auto;
        box-sizing: border-box;
        &-auto {
            -webkit-box-flex: 0;
            -ms-flex: 0 1 auto;
            flex: 0 1 auto;
        }
    }
}
.c-card {
    h2 {
        color: #217cb8;
    }
    hr {
        border: none;
        height: 1px;
        background-color: #555;
    }
}
.c-card__ul {
    margin: 0;
    text-align: left;
    padding: 0.5rem 0;
    list-style: none;
    box-sizing: border-box;
}
.c-card__ul-simple {
    margin: 0;
    text-align: left;
    padding: 1rem;
    list-style: none;
    li {
        box-sizing: border-box;
        padding: 0 0 0.5rem 0.75rem;
        position: relative;
        box-sizing: border-box;
        &:after {
            position: absolute;
            top: 0.5rem;
            left: 0;
            height: 0.25rem;
            width: 0.25rem;
            content: "";
            background: #000;
            border-radius: 50%;
        }
    }
}
.c-tile {
    display: block;
    height: 100%;
    width: 100%;
    max-width: 375px;
    margin: 0 auto;
    box-sizing: border-box;
    padding: 1.25rem 0.75rem;
    box-shadow: 0 .5rem .75rem 0 rgba(85,85,85,.5);
    transition: box-shadow .33s ease-out;
    text-decoration: none;
    &:hover {
        box-shadow: 0 .5rem .75rem 0 rgba(85,85,85,.75);
    }
    &__img {
        display: block;
        box-sizing: border-box;
        padding-bottom: 0.75rem;
        img {
            display: block;
            margin: 0 auto;
        }
    }
    &__headline {
        text-align: center;
        font-size: 1.5rem;
        color: #000;
    }
    &__text {
        text-align: center;
        color: #555;
    }
}
