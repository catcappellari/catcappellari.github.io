    }
}
.box-resume {
    h2 {
        padding-bottom: 0;
    }
    hr {
        height: 1px;
        border: none;
    }
    .hr-tall {
        height: 4px;
    }
    .logo-holder {
        display: block;
        text-decoration: none;
        background: #217cb8;
        max-width: 300px;
        margin: 0 auto;
        box-sizing: border-box;
        padding: 1rem;
        border: 6px solid #fff;
        box-shadow: 0px 0px 16px 0px rgba(0,0,0,0.75);
        h4 {
            padding: 1em 0 0;
            font-weight: 400;
            color: #fff;
            text-transform: uppercase;
            text-align: center;
        }
        img {
            width: 100%;
        }
    }
    .logo {
        display: block;
        max-width: 60px;
        margin: 0 auto;
        padding: 0;
    }
    .contact-info {
        padding: 1rem 0;
        margin: 0;
        list-style: none;
        text-align: center;
        p, a {
            color: #555;
            font-weight: bold;
            padding: 0;
        }
        a {
            text-decoration:none;
        }
        li {
            padding-bottom: 0.25rem;
        }
    }
    .sublist {
        box-sizing: border-box;
        padding: 0.5rem;
        -webkit-box-align: center;
        -ms-flex-align: center;
        align-items: center;
        border-bottom: 1px dashed rgba(0,0,0,0.25);
        p {
            padding: 0;
        }
        .c-flex__item {
            -ms-flex-preferred-size: 50%;
            flex-basis: 50%;
            &:nth-child(3) {
                text-align: right;
                p {
                    box-sizing: border-box;
                    padding-left: 0.25rem;
                }
                
            }
            &:nth-child(1) {
                p {
                    box-sizing: border-box;
                    padding-right: 0.25rem;
                } 
            }
        }
        .dotted {
            padding-bottom: 3px;
            position: absolute;
            display: none;
            &:after {
                position: absolute;
                bottom: 1px;
                height: 1px;;
                left: 0;
                right: 0;
                content: "";
                background-size: contain;
                background-repeat: repeat-x;
                background-image: url('../images/dashes.svg');
                opacity: 0.25;
            }
        }
    }
    .titlelist {
        .c-flex__item:nth-child(2) {
            text-align: right;
        }
        .c-flex__item:nth-child(1) {
            text-align: left;
        }
    }
}

@media screen and (min-width: 360px) {
    .c-buttons_ib {
        li {
            display: inline-block;
            vertical-align: top;
        }
    }
}

@media screen and (min-width: 552px) {
    .footer-main {
        .f-block {
            width: auto;
        }
        .social-media {
            text-align: right;
        }
    }
    .rocket-wrap {
        padding: 4rem 0;
        margin-left: 0;
    }
    .page-resume p {
        font-size: 1rem;
    }
    .page-resume li {
        font-size: 1rem;
    }
    .box-resume .c-card .dotted {
        display: block;
        position: relative;
    }
    .box-resume {
        .sublist {
            -webkit-box-align: stretch;
            -ms-flex-align: stretch;
            align-items: stretch;
            padding: 0;
            border-bottom: none;
            .c-flex__item {
                -ms-flex-preferred-size: auto;
                flex-basis: auto;
