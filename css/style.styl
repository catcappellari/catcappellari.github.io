/* Custom Styles */
.box-intro {
    font-size: 1.5rem;
    padding-top: 0.75rem;
}
.presentation {
    position: relative;
    &__wrapper {
        position: relative;
        z-index: 5;
        box-sizing: border-box;
    }
    &__wrapper_1 {
        padding: 2rem 1rem;
        img {
            display: block;
            max-width: 80%;
            margin: 0 auto;
        }
    }
    &__wrapper_2 {
        min-height: 15rem;
        background-color: #1f3a93;
        padding: 1.5rem 1rem 2rem;
        position: relative;
        margin-bottom: 70px;
        margin-top: 70px;
        &:before {
            position: absolute;
            height: 210px;
            width: 100%;
            right: 0;
            top: -50px;
            content: "";
            background: #1f3a93;
            -webkit-transform: skewY(-5deg);
            transform: skewY(-5deg);
            z-index: -1;
        }
        &:after {
            position: absolute;
            height: 210px;
            width: 100%;
            right: 0;
            bottom: -50px;
            content: "";
            background: #1f3a93;
            -webkit-transform: skewY(5deg);
            transform: skewY(5deg);
            z-index: -1;
        }
    }
}
.box-about {
    position: relative;
    &:before {
        position: absolute;
        height: 210px;
        width: 100%;
        right: 0;
        top: -25px;
        content: "";
        background: #217cb8;
        -webkit-transform: skewY(5deg);
        transform: skewY(5deg);
    }
    .about-svg {
        display: none;
    }
}
.footer-main {
    position: relative;
    padding-top: 1rem;
    .logo {
        display: block;
        max-width: 4rem;
        margin: 0 auto;
    }
    .social-media {
        text-align: center;
    }
    .c-buttons {
        padding-bottom: 1.5rem;
    }
    .c-flex__item {
        width: 100%;
    }
    &_minimal {
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        width: 100%;
        z-index: 6;
        .logo {
            max-width: 2rem;
        }
    }
}
.rocket-wrap {
    position: relative;
    box-sizing: border-box;
    padding: 4rem 0 2rem;
    width: 230px;
    margin: 0 auto;
    overflow: hidden;
    &:hover {
        .rocket-holder {
            transform:rotate(180deg);
        }
    }
}
.rocket-holder {
    display: block;
    position: absolute;
    height: 2rem;
    width: 230px;
    top: calc(50% - 16px);
    transform:rotate(0deg);
    transition: transform .5s linear;
    text-align: left;
}
.rocket {
    height: 32px;
    width: 32px;
    transform: rotate(-90deg);
    font-size: 2rem;
}
.rocket-button {
    border: 2px solid #fff;
    border-radius: 50%;
    display: block;
    box-sizing: border-box;
    padding: 1.25rem;
    width: 130px;
    height: 130px;
    max-width: 100%;
    margin: 0 auto;
    text-decoration: none;
    text-align: center;
    color: #217cb8;
    font-size: 18px;
    font-weight: bold;
    transition: all 0.3s linear;
    position: relative;
    z-index: 10;
    &:hover {
        background: #217cb8;
        color: #fff;
        border-color:  #217cb8;
    }
    span {
        position: absolute;
        width: 100%;
        top: 50%;
        left: 0;
        -webkit-transform: translateY(-50%);
        transform: translateY(-50%);
    }
}

/* interior pages */
.ux-slide {
    box-sizing: border-box;
    padding: 1rem;
    img {
        margin: 0 auto;
    }
}
.page-resume {
    p {
        font-size: 0.9rem;
    }
    li {
