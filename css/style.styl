/* Neon */
.landing-neon {
    .box-neon {
        background-image: url('../images/bg-neon.jpg');
        height: 100vh;
        &:after {
            background: rgba(0,0,0,0.66);
        }
    }
    .footer-main {
        p {
            box-sizing: border-box;
            padding: 0.75rem 0.5rem;
            text-align: center;
            color: #fff;
        }
    }
}
.e-neon {
    box-sizing: border-box;
    padding: 1rem;
    position: relative;
    &:before {
        position: absolute;
        content: "";
        height: 6px;
        width: 6px;
        border-radius: 0 0 0 12px;
        border-left: 6px solid #ff6bff;
        box-sizing: border-box;
        overflow: hidden;
        border-bottom: 6px solid #ff6bff;
        bottom: 14px;
        left: 16px;
        opacity: 0;
        transition: width 0s linear .25s;
        box-shadow: 0 0 .75em 1px #ff6bff;
    }
    &:after {
        position: absolute;
        content: "";
        height: 6px;
        width: 6px;
        border-radius: 0 0 12px 0;
        border-right: 6px solid #ff6bff;
        border-bottom: 6px solid #ff6bff;
        box-sizing: border-box;
        overflow: hidden;
        bottom: 14px;
        right: 16px;
        opacity: 0;
        transition: opacity 0s linear .25s;
        box-shadow: 0 0 .75em 1px #ff6bff;
        
    }
    &__headline {
        color: #ff6bff;
        text-shadow: 0 0 .25em rgba(255, 107, 255, 1);
        font-family: 'Comfortaa', 'Century Gothic', CenturyGothic, AppleGothic, sans-serif;
        box-sizing: border-box;
        padding: 2rem 1rem 1.5rem 1rem;
        line-height: 1;
        font-size: 1.5rem;
        span {
            opacity: 0;
            
        }
    }
    &__horizontal {
        position: relative;
        &:before {
            position: absolute;
            content: "";
            height: 6px;
            width: 0;
            top: 2px;
            right: 6px;
            background: #ff6bff;
            transition: width .5s linear .5s;
            box-shadow: 0 0 .75em 1px #ff6bff;
        }
        &:after {
            position: absolute;
            content: "";
            height: 6px;
            width: 0;
            bottom: -2px;
            left: 6px;
            background: #ff6bff;
            transition: width .25s linear;
            box-shadow: 0 0 .75em 1px #ff6bff;
        }
    }
    &__vertical {
        position: relative;
        &:before {
            position: absolute;
            content: "";
            width: 6px;
            height: 0;
            top: 8px;
            left: 0;
            background: #ff6bff;
            transition: height .5s linear 1s;
            box-shadow: 0 0 .75em 1px #ff6bff;
            -webkit-animation: flash 4s alternate 2s infinite;
            animation: flash 4s alternate 2s infinite;
        }
        &:after {
            position: absolute;
            content: "";
            width: 6px;
            height: 0;
            bottom: 4px;
            right: 0;
            background: #ff6bff;
            transition: height .25s linear .25s;
            box-shadow: 0 0 .75em 1px #ff6bff;
        }
    }
    &__corners {
        position: relative;
        &:before {
            position: absolute;
            content: "";
            height: 6px;
            width: 6px;
            border-radius: 0 12px 0 0;
            border-left: 6px solid #ff6bff;
            box-sizing: border-box;
            overflow: hidden;
            border-bottom: 6px solid #ff6bff;
            top: 2px;
            right: 0;
            opacity: 0;
            transition: opacity 0s linear .5s;
            box-shadow: 0 0 .75em 1px #ff6bff;
        }
        &:after {
            position: absolute;
            content: "";
            height: 6px;
            width: 6px;
            border-radius: 12px 0 0 0;
            border-right: 6px solid #ff6bff;
            border-bottom: 6px solid #ff6bff;
            box-sizing: border-box;
            overflow: hidden;
            top: 2px;
            left: 0;
            opacity: 0;
            transition: opacity 0s linear 1s;
            box-shadow: 0 0 .75em 1px #ff6bff;
        }
    }
    &.is-active {
        span {
            &:nth-child(1) {
                -webkit-animation: flash 2s linear 1s forwards;
                animation: flash 2s linear 1s forwards;
            }
            &:nth-child(2) {
                -webkit-animation: flash 7s forwards 5s infinite;
                animation: flash 7s forwards 5s infinite;
            }
            &:nth-child(3) {
                -webkit-animation: flash 2s linear 3s forwards;
                animation: flash 2s linear 3s forwards;
            }
            &:nth-child(4) {
                -webkit-animation: flash 3s linear 4s forwards;
                animation: flash 3s linear 4s forwards;
            }
            &:nth-child(5) {
                -webkit-animation: flash 2s linear 5.5s forwards;
                animation: flash 2s linear 5.5s forwards;
            }
            &:nth-child(6) {
                -webkit-animation: flash 2s linear 6s forwards;
                animation: flash 2s linear 6s forwards;
            }
            &:nth-child(7) {
                -webkit-animation: flash 2s linear 6.75s forwards;
                animation: flash 2s linear 6.75s forwards;
            }
            &:nth-child(8) {
                -webkit-animation: flash 2s linear 5.5s forwards;
                animation: flash 2s linear 5.5s forwards;
            }
            &:nth-child(9) {
                -webkit-animation: flash 2s linear 7.25s forwards;
                animation: flash 2s linear 7.25s forwards;
            }
            &:nth-child(10) {
                -webkit-animation: flash 6s forwards 8s infinite;
                animation: flash 6s forwards 8s infinite;
            }
            &:nth-child(11) {
                -webkit-animation: flash 2s linear 8.5s forwards;
                animation: flash 2s linear 8.5s forwards;
            }
        }
        &:before {
            opacity: 1;
        }
        &:after {
            opacity: 1;
        }
        .e-neon__corners {
            &:before {
                opacity: 1;
            }
            &:after {
                opacity: 1;
            }
        }
        .e-neon__horizontal:before {
            width: calc(100% - 12px);
        }
        .e-neon__horizontal:after {
            width: calc(100% - 12px);
        }
        .e-neon__vertical:before {
            height: calc(100% - 12px);
        }
        .e-neon__vertical:after {
            height: calc(100% - 12px);
        }
    }
}

