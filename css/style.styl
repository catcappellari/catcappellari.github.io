@-webkit-keyframes flash {
    0%   { opacity: 0.1; }
    5%   { opacity: 1; }
    10%   { opacity: 0.25; }
    50%   { opacity: 1; }
    66%   { opacity: 0.75; }
    100% { opacity: 1; }
}

@keyframes flash {
    0%   { opacity: 0.1; }
    5%   { opacity: 1; }
    10%   { opacity: 0.25; }
    50%   { opacity: 1; }
    66%   { opacity: 0.75; }
    100% { opacity: 1; }
}

